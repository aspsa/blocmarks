<h4><%= link_to "Back to Topics", topics_path %></h4>
<h1>Bookmark Topic: <%= @topic.title %></h1>
<h4><%= link_to "Add a Bookmark", new_topic_bookmark_path(@topic) %></hr>

<ul>
  <% @topic.bookmarks.each do |b| %>
    <li>
      <%= b.url %> | [<%= link_to "Edit", '#' %>][<%= link_to "Delete", '#' %>]
    </li>
  <% end %>
</ul>